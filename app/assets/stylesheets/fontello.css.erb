@font-face {
  font-family: 'fontello';
  src: url('<%= asset_path ('font/fontello.eot?30724885') %>');
  src: url('<%= asset_path('font/fontello.eot') %>?30724885#iefix') format('embedded-opentype'),
       url('<%= asset_path ('font/fontello.woff') %>?30724885') format('woff'),
       url('<%= asset_path ('font/fontello.ttf') %>?30724885') format('truetype'),
       url('<%= asset_path ('font/fontello.svg') %>?30724885#fontello') format('svg');
  font-weight: normal;
  font-style: normal;
}
/* Chrome hack: SVG is rendered more smooth in Windozze. 100% magic, uncomment if you need it. */
