    <link href='http://fonts.googleapis.com/css?family=Lato:300,400,700' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,300' rel='stylesheet' type='text/css'>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body id="page-top" class="home bg-color-1" data-spy="scroll">
    <!-- PAGE WRAP -->
    <div id="page-wrap">

        <!-- PRELOADER -->
        <div class="preloader">
            <div class="pre-icon">
                <div class="pre-item pre-item-1"></div>
                <div class="pre-item pre-item-2"></div>
                <div class="pre-item pre-item-3"></div>
                <div class="pre-item pre-item-4"></div>
            </div>
        </div>
        <!-- END / PRELOADER -->

        <!-- HEADER -->
        <header id="header" class="header">
            <!-- BANNER -->
            <div id="banner">
                
            </div>
            <!-- END / BANNER -->

            <div class="container">

                <!-- LOGO -->
                <div class="logo text-center"><a href="index.html"><%= image_tag("logo.png", :alt => "Nicolas Candelon développeur ruby on rails à Agen") %></a></div>
                <!-- END / LOGO -->

                <!-- HEAD CONTENT -->
                <div class="head-content text-center">
                    <div class="head-title text-uppercase">
                        <p>Nicolas Candelon</p><br/>
                        <h1 class="rotate">Intégrateur, Développeur Ruby on rails</h1>
                        <hr class="line">
                    </div>
                    <p>Freelance à Agen dans le Lot et Garonne, je mets ma passion à disposition pour vous accompagner au mieux dans la réussite de votre projet web.</p>
                </div>
                <!-- END / HEAD CONTENT -->
            </div>

            <div class="cs-scroll">
                <i class="fa fa-angle-down"></i>
                <div class="line-1"></div>
                <div class="line-2"></div>
                <div class="line-3"></div>
                <div class="line-4"></div>
            </div>
                
        </header>
        <!-- END / HEADER -->

        <!-- NAVIGATION -->
        <nav class="navigation">
            <div class="logo-nav text-center"><a href="index.html"><img src="images/logo.png" alt=""></a></div>
            
            <ul class="nav text-center text-uppercase">
                <li><a href="#page-top" data-title="Home">Home</a></li>
                <li><a href="#about" data-title="About">About</a></li>
                <li><a href="#work" data-title="Portfolio">Portfolio</a></li>
                <li><a href="#service" data-title="Service">Service</a></li>
                <li><a href="#team" data-title="Team">Team</a></li>
                <li><a href="#blog-time" data-title="Blog">Blog</a></li>
            </ul>
        </nav>
        <!-- END / NAVIGATION -->

        <!-- CONTACT AND SOCIAL -->
        <div class="page-right-nav">
            <div class="contact-slide">
                <h4 class="open-pp-contact">Contact</h4>

                <div class="pp-contact">
                    <div class="inner">
                        <h5>Contact</h5>
                        <div id="map"></div>

                        <div class="address">
                            <div class="half">
                                <i class="icon md-icon-location"></i>
                                <ul>
                                    <li>Office #01</li>
                                    <li>42 Maverick Street, Boston, MA 02128</li>
                                    <li>support@themestreet.net</li>
                                    <li>1-855-289-5858</li>
                                </ul>
                            </div>
                            <div class="half">
                                <i class="icon md-icon-location"></i>
                                <ul>
                                    <li>Office #02</li>
                                    <li>42 Maverick Street, Boston, MA 02128</li>
                                    <li>support@themestreet.net</li>
                                    <li>1-855-289-5858</li>
                                </ul>
                            </div>
                        </div>

                        <div class="contact-form">
                            <form id="send-message-form" action="processContact.php" method="post">
                                <div class="half">
                                    <textarea class="message" name="message" onfocus="if(this.value == 'Give us a message or just say hello') { this.value = ''; }" onblur="if(this.value == '') { this.value = 'Give us a message or just say hello'; }">Give us a message or just say hello</textarea>
                                </div>
                                <div class="half">
                                    <input class="email" type="text" name="email" value="Email Address" onfocus="if(this.value == 'Email Address') { this.value = ''; }" onblur="if(this.value == '') { this.value = 'Email Address'; }">
                                    <input class="name" type="text" name="name" value="Your Lovely Name" onfocus="if(this.value == 'Your Lovely Name') { this.value = ''; }" onblur="if(this.value == '') { this.value = 'Your Lovely Name'; }">
                                    <!-- <input type="submit" value="SEND MESSAGE" class="cs-btn btn-md btn-style-1 submit text-uppercase"> -->
                                    <button id="submit-contact" class="cs-btn btn-md btn-style-1 submit text-uppercase" type="submit">Send Message</button>
                                </div>
                            </form>

                        </div>
                        <div id="contact-content"></div>
                    </div>
                </div>
            </div>

            <div class="social">
                <a href="#"><i class="fa fa-dribbble"></i></a>
                <a href="#"><i class="fa fa-facebook"></i></a>
                <a href="#"><i class="fa fa-instagram"></i></a>
                <a href="#"><i class="fa fa-tumblr"></i></a>
                <a href="#"><i class="fa fa-twitter"></i></a>
            </div>
        </div>
        <!-- END / CONTACT AND SOCIAL -->



        <!-- END / CONTACT AND SOCIAL -->

    		<%= yield %>

        <!-- FOOTER -->
        <footer id="footer" class="footer">
            <div class="section-wrap bg-parallax">
                <div class="container">
                    <div class="title">
                        <h3 class="lg text-uppercase">BE OUR FRIEND <span class="text-lowercase">and</span> KEEP IN TOUCH</h3>
                    </div>

                    <div class="foot-social">
                        <a class="wow zoomIn" data-wow-delay="0.2s" href="#"><i class="icon icon-twitter"></i></a>
                        <a class="wow zoomIn" data-wow-delay="0.3s" href="#"><i class="icon icon-tumblr"></i></a>
                        <a class="wow zoomIn" data-wow-delay="0.4s" href="#"><i class="icon icon-dribbble"></i></a>
                        <a class="wow zoomIn" data-wow-delay="0.5s" href="#"><i class="icon icon-google"></i></a>
                        <a class="wow zoomIn" data-wow-delay="0.6s" href="#"><i class="icon icon-pinterest"></i></a>
                        <a class="wow zoomIn" data-wow-delay="0.7s" href="#"><i class="icon icon-facebook"></i></a>
                    </div>
                </div>

                <p class="foot-text">If you expect something so so, Don’t hire us! we just do the outstanding</p>

                <p class="copyright">Designed by MegaDrupal @ 2014</p>
                <div class="overlay-pattern-2"></div>
            </div>
        </footer>
        <!-- END / FOOTER -->


    </div>
<!-- END / PAGE WRAP -->

