<script type="text/javascript">
    "use strict";
    /*==============================
        Mobile check
    ==============================*/
    function mobilecheck() {
        if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
            return false;
        }
        return true;
    }

    /*==============================
        ANIMATION
    ==============================*/
    if (mobilecheck()) {
        new WOW().init();   
    }
</script>
</body>
</html>